*********************************************************************
*** vertex-symbols and vertex-functions *****************************
*********************************************************************

* Generic vertex functions

cf vertVV,vertSS,vertVS;
cf vertVVV,vertVVVV,vertSVV,vertVVSS,vertVSS,vertVGG;

*** Model-specific definitions ***
#do i = 1, `NM'
  #include model_`MODEL`i'' # DEF
#enddo


******************************************************************
*** Sets for generating vertices *********************************
******************************************************************
*                                                                *
* depending on the set they appear these functions and their     *
* corresponding vertices will be treated differently             *
* the ordering in the sets is important                          *
*                                                                *
******************************************************************

cf Ff;
set fermion:
Ff,
#do i = 1, `NM'
  #include model_`MODEL`i'' # FF
#enddo
;

s vv;
set vertices:
vv,
#do i = 1, `NM'
  #include model_`MODEL`i'' # VERTICES
#enddo
;

