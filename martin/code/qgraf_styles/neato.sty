<prologue>
<diagram>
graph "<diagram_index>" {
  graph [[overlap=scale]];
  node [[shape=point]];
  label="Diagram <diagram_index> (factor = <sign><symmetry_factor>)";

  <back><in_loop>
  in<in_index>--<vertex_index> 
  [[label="<field>(<momentum>)",
    dir=forward,
    fontcolor=<field_sign>,
    color=<field_sign>]];
  <back><end>

  <back><out_loop>
  <vertex_index>--out<out_index> 
  [[label="<field>(<momentum>)",
    dir=forward,
    fontcolor=<field_sign>,
    color=<field_sign>]];
  <back><end>

  <back><propagator_loop>
  <dual-vertex_index>--<vertex_index>
  [[label="<field>",
    dir=forward,
    fontcolor=<field_sign>,
    color=<field_sign>]];
  <back><end>
}
<epilogue>
<exit>

